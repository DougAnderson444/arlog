{
	"name": "@douganderson444/arlog",
	"version": "0.2.1",
	"scripts": {
		"dev": "svelte-kit dev --port=34779",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"package": "svelte-kit package",
		"publish": "npm publish ./dist",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"build:ipfs": "esbuild ./node_modules/ipfs-core --bundle --format=esm --sourcemap --main-fields=browser,module,main --inject:./src/node-globals.js --define:process.env.NODE_ENV='\"production\"' --splitting --outdir=./src/modules/ipfs-core"
	},
	"devDependencies": {
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.4.1",
		"micromatch": "^4.0.4",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"rollup-plugin-string": "^3.0.0",
		"svelte": "^3.34.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
		"svelte2tsx": "^0.4.10",
		"sveltejs-adapter-ipfs": "^0.1.10",
		"testweave-sdk": "^0.2.2",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@lukeed/uuid": "^2.0.0",
		"arweave": "^1.10.18",
		"buffer": "^5.7.1",
		"cookie": "^0.4.1",
		"esbuild": "^0.13.15",
		"immortal-db": "^1.1.0",
		"ipfs-core": "^0.12.1",
		"process": "^0.11.10",
		"smartweave": "^0.4.46"
	}
}
